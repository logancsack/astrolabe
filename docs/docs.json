          },
          {
            "group": "Using Astrolabe",
            "pages": [
              "openclaw",
              "deploy/railway"
            ]
          },
          {
            "group": "Operations",
            "pages": [
              "model-roster",
              "routing-reference",
              "configuration",
              "troubleshooting"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/introduction",
              "api-reference/endpoint/chat-completions"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/astrolabe-horiz-light.png",
    "dark": "/logo/astrolabe-horiz-dark.png"
